{
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "extends": ["airbnb-typescript-prettier"],
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    },
    "rules": {
        "import/prefer-default-export": "off",
        "no-console": "off",
        "jsx-a11y/click-events-have-key-events": 0,
        "jsx-a11y/no-static-element-interactions": 0
    }
}